{
  "name": "kahlillechelt.com",
  "version": "1.0.0",
  "description": "My personal site.",
  "main": "index.js",
  "scripts": {
    "css": "postcss -u postcss-import postcss-cssnext postcss-clean --no-map css/styles.css -o static/css/styles.min.css",
    "watch:css": "postcss -u postcss-import postcss-cssnext postcss-clean --no-map css/styles.css -o static/css/styles.min.css -w",
    "start": "hugo serve & yarn watch:css",
    "start:drafts": "hugo serve -D & yarn watch:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahlil/kahlillechelt.com.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/kahlil/kahlillechelt.com/issues"
  },
  "homepage": "https://github.com/kahlil/kahlillechelt.com#readme",
  "dependencies": {
    "caniuse-lite": "1.0.30001062",
    "chokidar-cli": "2.1.0",
    "postcss": "7.0.32",
    "postcss-clean": "1.1.0",
    "postcss-cli": "7.1.1",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "12.0.1"
  },
  "volta": {
    "node": "12.16.3",
    "yarn": "1.22.4"
  }
}
